    <body>
        <h1>Edit pokemon</h1>
        <form action="/pokemon/create" method="post">
            <label for="pokemonName">Name: </label>
            <input type="text" name="pokemonName" value="{{data.name}}"><br>
            <br>
            <label for="pokemonLevel">Level: </label>
            <input type="text" name="pokemonLevel" value="{{data.level}}"><br>
            <br>
            <input type="submit" value="submit">
        </form>
        <br>
         <button onclick="window.history.back()">Back</button>

    </body>
</html>